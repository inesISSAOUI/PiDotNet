@model IEnumerable<test_data.niveauxmat>
@using Microsoft.AspNet.Identity


<div class="box box-primary">
    <div class="box-body box-profile">
        <img class="profile-user-img img-responsive img-circle" src="~/fonts/houssem.jpg" alt="User profile picture">

        <h3 class="profile-username text-center">@ViewBag.employe.nom</h3>

        <p class="text-muted text-center">Software Engineer</p>

        <ul class="list-group list-group-unbordered">
            <li class="list-group-item">
                <b>@ViewBag.employe.title</b><a class="pull-right">title</a>
            </li>
            <li class="list-group-item">
                <b> @ViewBag.id</b><a class="pull-right">Nombre des Competance</a>
            </li>



    </div>
    <!-- /.box-body -->
</div>
<a href="@Url.Action("ListComp","employes")" class="btn btn-primary btn-block"><b>Voir Liste des Compteance</b></a>
<a href="@Url.Action("demande","employes")" class="btn btn-primary btn-block"><b>demander promotion</b></a>
